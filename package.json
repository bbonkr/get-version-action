{
  "name": "get-version-action",
  "version": "1.0.3",
  "description": "This is an action to get the version string from project file such as package.json, .csproj (c# sdk style project file)",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbonkr/get-version-action.git"
  },
  "keywords": [
    "actions",
    "github",
    "version",
    "npm",
    "nodejs",
    "dotnet"
  ],
  "author": {
    "email": "dev@bbon.kr",
    "name": "Pon Cheol Ku (구본철)",
    "url": "https://bbon.kr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbonkr/get-version-action/issues"
  },
  "homepage": "https://github.com/bbonkr/get-version-action#readme",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@xmldom/xmldom": "^0.8.1",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/parser": "^5.14.0",
    "@vercel/ncc": "^0.33.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.3"
  }
}
